{
  "name": "dayclap-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "dev": "vite --host",
    "backend": "python3 backend/app.py",
    "start-all": "concurrently \"npm run backend\" \"npm run dev\"",
    "build": "vite build",
    "prebuild": "npm rebuild esbuild || true",
    "postinstall": "npm rebuild esbuild || true",
    "preview": "vite preview",
    "gen:vapid": "node scripts/generateVapidKeys.cjs",
    "gen:vapid:esm": "node scripts/generateVapidKeys.js",
    "sanitize:dashboard": "node scripts/sanitize-dashboard.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.43.4",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.4.5",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.1",
    "web-push": "^3.6.6"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.0.3",
    "concurrently": "^8.2.0",
    "esbuild": "^0.19.12",
    "vite": "^4.4.5",
    "vite-plugin-pwa": "^0.16.4"
  },
  "optionalDependencies": {
    "@esbuild/linux-x64": "0.19.12"
  }
}
